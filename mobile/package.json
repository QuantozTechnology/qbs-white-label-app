{
  "name": "mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "ci-test": "TZ=UTC jest --ci --maxWorkers=2 --forceExit --reporters=\"jest-junit\" --reporters=\"default\" --silent",
    "test": "TZ=UTC npx jest --forceExit",
    "lint": "eslint",
    "prepare": "cd .. && husky install",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "create-stores-build": "eas build --profile preview-store  --auto-submit",
    "update-stores-build": "eas update --branch preview-store"
  },
  "dependencies": {
    "@expo/config-plugins": "~6.0.0",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/datetimepicker": "6.7.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/material-top-tabs": "^6.5.1",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@react-navigation/stack": "^6.3.2",
    "@tanstack/react-query": "^4.29.1",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.34",
    "axios": "^1.1.3",
    "expo": "^48.0.10",
    "expo-auth-session": "~4.0.3",
    "expo-barcode-scanner": "~12.3.2",
    "expo-camera": "~13.2.1",
    "expo-clipboard": "~4.1.2",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-font": "~11.1.1",
    "expo-image-picker": "~14.1.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-linking": "~4.0.1",
    "expo-local-authentication": "~13.3.0",
    "expo-mail-composer": "~12.1.1",
    "expo-secure-store": "~12.1.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.5.0",
    "jest-expo": "^48.0.2",
    "jest-junit": "^15.0.0",
    "jwt-decode": "^3.1.2",
    "native-base": "~3.4.13",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.7",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-mask-input": "^1.2.2",
    "react-native-pager-view": "6.1.2",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "~2.14.4",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-tab-view": "^3.3.4",
    "typescript": "^4.9.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-testing-library": "^5.6.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "msw": "^1.2.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.0.0",
    "ts-node": "^10.9.1"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint \"./src/\" --fix --max-warnings 0"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": false,
    "endOfLine": "lf"
  },
  "resolutions": {
    "@types/react": "^18"
  }
}
