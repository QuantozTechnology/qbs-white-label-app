  "private": true,
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint \"./src/\" --fix --max-warnings 0"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": false,
    "endOfLine": "lf"
  },
  "resolutions": {
    "@types/react": "^18"
  }
