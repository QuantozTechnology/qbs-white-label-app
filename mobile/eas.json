{
  "cli": {
    "version": ">= 2.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "development"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel":"test",
      "distribution": "store",
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "test": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6446848462"
      }
    }
  }
}
